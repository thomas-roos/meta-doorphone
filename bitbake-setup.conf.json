                    "meta-raspberrypi"
                ],
                "bb-layers-file-relative": [
                    "meta-doorphone"
                ],
				"oe-fragments": ["machine/raspberrypi-armv8", "doorphone/doorphone"],
                "oe-fragments-one-of": {
                    "distro": {
